{
	"name": "<%= appName %>",
	"version": "0.0.0",
	"homepage": "https://github.com/<%= githubName%>/<%= appName %>",
	"authors": [
		"<%= authorName %>"
	],
	"description": "<%= appDescription %>",
	"keywords": [
		<% for(var ii=0; ii<appKeywords.length; ii++) {
			if(ii !==0) {
				print('\t\t');
			}
			print('"'+appKeywords[ii]+'"');
			if(ii !==(appKeywords.length -1)) {
				print(',\n');
			}
		}
		%>
	],
	"dependencies": {
		"angular":"1.2.0-rc.3",
		"angular-animate":"1.2.0-rc.3",
		"angular-sanitize":"1.2.0-rc.3",
		"angular-touch":"1.2.0-rc.3",
		"angular-route":"1.2.0-rc.3",
		"angular-cookies":"1.2.0-rc.3",
		"lesshat":"latest",
		"font-awesome":"latest",
		"less-flexbox":"latest",
		"momentjs":"latest",
		"hammerjs":"latest",
		"angular-hammer":"latest",
		"angular-string":"latest",
		"angular-array":"latest",
		"angular-facebook-auth":"latest",
		"angular-google-auth":"latest",
		"angular-forminput":"latest"
	},
	"license": "MIT"
}
